mtdiscrete ntgcd( mtdiscrete m, mtdiscrete n )

MAX (d IN NN(m > n ? n : m), d > 0, mod(m,d) == 0 AND mod(n,d) == 0)

END

mtbool ntrelprime( mtdiscrete m, mtdiscrete n )

FORALL (d IN NN(m > n ? n+1 : m+1), d > 1, mod(m,d) != 0 OR mod(n,d) != 0)

END

mtdiscrete ntphi( mtdiscrete n )

COUNT (d IN NN(n+1), d > 0, ntrelprime( n,d ))

END




mtset nWalkss( mtdiscrete u, mtdiscrete v, mtdiscrete length )

SET (m IN Maps(length+1, dimm),
     FORALL (n IN st(m) - 1, ac(m[n],m[n+1])) AND m[0] == u AND m[st(m)-1] == v, m )

END

mtset Hpaths( mtset HV, mtset HE )

BIGCUP (u IN HV, v IN HV, u < v, SET (P IN Pathss(u,v), P CAP HV == {u,v} AND ({u,v} ELT HE IMPLIES st(P) > 2), P))

END

/* When in service of Tpartialorder and TnormalinG, the need to check whether
   the edge is in the subgraph is obviated, since always they are then comparable in the Torder */

mtset Hpathsquick( mtset HV )

BIGCUP (u IN HV, v IN HV, u < v, SET (P IN Pathss(u,v), P CAP HV == {u,v}, P))

END


mtbool Tpartialorder( mtdiscrete root, mtset T, mtdiscrete u, mtdiscrete v )

u ELT pyTdownclosure(root, T, v)

END

mtbool TnormalinG( mtdiscrete root, mtset T )

NAMING (TV AS BIGCUP(e IN T, e),
    FORALL (p IN Hpathsquick(TV), Tpartialorder( root, T, p[0], p[st(p)-1] ) OR Tpartialorder( root, T, p[st(p)-1], p[0] )))

END

mtset treefromorderedvertices( mttuple O )

SET (n IN NN(dimm), n >= 1, {ANY (m IN NN(n), ac(O[n],O[m]), O[m]),O[n]} )

END

